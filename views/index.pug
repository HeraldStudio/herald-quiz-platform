extends layout

block content
  link(rel='stylesheet', href='/stylesheets/index.css')
  .img_btn
    img(src='/images/SEU_Logo.svg')
    .blank_img
    .rights-reserved
      table(cellpadding="4")
        tr
          td 主办方:
          td 东南大学党委研工部
        tr
          td 承办方:
          td 研究生党支部联合中心
        tr
          td 技术支持:
          td 计算机科学与工程学院
        tr
          td
          td
            a(href="https://myseu.cn/#/") #小猴偷米工作室
    .btn-group-vertical
      a(href='/register' class='btn btn-primary', role='button') 注册
      .blank_btn
      a(href='/login' class='btn btn-primary', role='button') 登录
    - if (error)
      div(class='alert alert-danger', role='alert')
        strong #{error}
    - if (success)
      div(class='alert alert-success', role='alert')
        strong #{success}
